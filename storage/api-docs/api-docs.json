{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel API Auth",
        "version": "1.0.0"
    },
    "paths": {
        "/api/keys/create": {
            "post": {
                "tags": [
                    "API Keys"
                ],
                "summary": "Create new API key",
                "operationId": "7f1ca4c19dab3c8aed969a5b10ba6c73",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "permissions",
                                    "expiry"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "wallet-service"
                                    },
                                    "permissions": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "enum": [
                                                "deposit",
                                                "transfer",
                                                "read"
                                            ]
                                        }
                                    },
                                    "expiry": {
                                        "description": "1H, 1D, 1M, or 1Y",
                                        "type": "string",
                                        "example": "1D"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "API key created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "properties": {
                                                "api_key": {
                                                    "description": "SAVE THIS! It's shown only once",
                                                    "type": "string"
                                                },
                                                "expires_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/keys/rollover": {
            "post": {
                "tags": [
                    "API Keys"
                ],
                "summary": "Rollover expired API key",
                "operationId": "a10fd5469582f87e8c5e37222e22cc94",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "expired_key_id",
                                    "expiry"
                                ],
                                "properties": {
                                    "expired_key_id": {
                                        "type": "string"
                                    },
                                    "expiry": {
                                        "type": "string",
                                        "example": "1M"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "API key rolled over"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/keys": {
            "get": {
                "tags": [
                    "API Keys"
                ],
                "summary": "List all API keys",
                "operationId": "bd82250cacaf0cb5e68f5fb5f41ef508",
                "responses": {
                    "200": {
                        "description": "List of API keys"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/keys/{id}": {
            "delete": {
                "tags": [
                    "API Keys"
                ],
                "summary": "Revoke API key",
                "operationId": "57329fb496fcb6b78d89590dd620112a",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "API key revoked"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/signup": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register a new user",
                "operationId": "887d1dd2c4a5b34ac964b762c9555432",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User created successfully"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user and get JWT",
                "operationId": "8dcb70df1020986038d098cc08d05dae",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "401": {
                        "description": "Invalid credentials"
                    }
                }
            }
        },
        "/api/me": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Get authenticated user profile",
                "operationId": "9d9bc556f927575aa492a88c28fe5908",
                "responses": {
                    "200": {
                        "description": "User profile"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout user",
                "operationId": "fe8f3429cd6979b3b4517e186505f9f9",
                "responses": {
                    "200": {
                        "description": "Logged out successfully"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/refresh": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Refresh JWT token",
                "operationId": "2c27d311a2876560fcb5a754b556cdc5",
                "responses": {
                    "200": {
                        "description": "New token generated"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/google": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Redirect to Google OAuth",
                "description": "Click the link below to login with Google. DO NOT use Try it out.",
                "externalDocs": {
                    "description": "LOGIN WITH GOOGLE HERE",
                    "url": "/api/auth/google"
                },
                "operationId": "d9545bf39a43c7b6dad24d8b6b4bd67d",
                "responses": {
                    "302": {
                        "description": "Redirect to Google"
                    }
                }
            }
        },
        "/api/auth/google/callback": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Handle Google OAuth callback",
                "operationId": "5793de99cbbfc80402e9b1663438cf52",
                "responses": {
                    "200": {
                        "description": "Login successful with JWT"
                    }
                }
            }
        },
        "/api/wallet/transfer": {
            "post": {
                "tags": [
                    "Transfer"
                ],
                "summary": "Transfer to another wallet",
                "operationId": "10cc1674827d231cf759a964377d8e86",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "wallet_number",
                                    "amount"
                                ],
                                "properties": {
                                    "wallet_number": {
                                        "type": "string",
                                        "example": "45123456789012"
                                    },
                                    "amount": {
                                        "type": "number",
                                        "example": 3000
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Transfer successful",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "properties": {
                                                "reference": {
                                                    "type": "string"
                                                },
                                                "amount": {
                                                    "type": "number"
                                                },
                                                "recipient_wallet": {
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    },
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/wallet/balance": {
            "get": {
                "tags": [
                    "Wallet"
                ],
                "summary": "Get wallet balance",
                "operationId": "2a4a1c7eba45be583fce194ae60a72c1",
                "responses": {
                    "200": {
                        "description": "Wallet balance",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "properties": {
                                                "balance": {
                                                    "type": "number",
                                                    "example": 15000
                                                },
                                                "currency": {
                                                    "type": "string",
                                                    "example": "NGN"
                                                },
                                                "wallet_number": {
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    },
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/wallet/deposit": {
            "post": {
                "tags": [
                    "Wallet"
                ],
                "summary": "Initialize deposit",
                "operationId": "506845afd12f94e48ec2a586dfb9ee86",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "amount"
                                ],
                                "properties": {
                                    "amount": {
                                        "description": "Amount in Naira",
                                        "type": "number",
                                        "example": 5000
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Deposit initialized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "properties": {
                                                "reference": {
                                                    "type": "string"
                                                },
                                                "authorization_url": {
                                                    "description": "Paystack checkout URL",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    },
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/wallet/deposit/{reference}/status": {
            "get": {
                "tags": [
                    "Wallet"
                ],
                "summary": "Check deposit status",
                "operationId": "7a3714a82b33fe3f3c9d2c594c5e9223",
                "parameters": [
                    {
                        "name": "reference",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Transaction status"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    },
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/wallet/transactions": {
            "get": {
                "tags": [
                    "Wallet"
                ],
                "summary": "Get transaction history",
                "operationId": "d7003c8f7d8d61b2b6b5b2d6f2377cc3",
                "responses": {
                    "200": {
                        "description": "List of transactions"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    },
                    {
                        "apiKey": []
                    }
                ]
            }
        },
        "/api/wallet/paystack/webhook": {
            "post": {
                "tags": [
                    "Webhooks"
                ],
                "summary": "Paystack webhook handler",
                "operationId": "a67c80c935e0ffe58d6f06fe50397b4e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "event": {
                                        "type": "string",
                                        "example": "charge.success"
                                    },
                                    "data": {
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Webhook processed"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            },
            "apiKey": {
                "type": "apiKey",
                "description": "API Key Authentication",
                "name": "x-api-key",
                "in": "header"
            }
        }
    },
    "tags": [
